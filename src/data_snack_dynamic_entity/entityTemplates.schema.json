{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "patternProperties": {
    "^[A-Z][a-zA-Z0-9]*$": {
      "type": "object",
      "properties": {
        "properties": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z0-9_]+$": {
              "anyOf": [
                {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string"
                    },
                    "default": {
                      "type": [
                        "string",
                        "number",
                        "boolean"
                      ]
                    },
                    "key": {
                      "type": "boolean",
                      "enum": [false]
                    },
                    "optional": {
                      "type": "boolean",
                      "default": false
                    },
                    "excluded": {
                      "type": "boolean",
                      "default": false
                    }
                  },
          		  "additionalProperties": false
                },
                {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string"
                    },
                    "key": {
                      "type": "boolean",
                      "enum": [true]
                    },
                    "optional": {
                      "type": "boolean",
                      "default": false,
                      "enum": [false]
                    },
                    "excluded": {
                      "type": "boolean",
                      "default": false,
                      "enum": [false, true]
                    }
                  },
          		  "additionalProperties": false
                }
              ]
            }
          },
          "additionalProperties": false
        }
      },
      "required": [
        "properties"
      ]
    }
  },
  "additionalProperties": false
}